---
import ServiceLayout from "../Layouts/ServiceLayout.astro";
import Site from "../components/Site.json";

import img1 from "/assets/supressao/1.png";
import img2 from "/assets/supressao/2.png";
import img3 from "/assets/supressao/3.png";
import img4 from "/assets/supressao/4.jpg";

import SupressaoContent from "../content/SupressaoContent.astro";

const data = Site.items.find((item) => item.id === 3);
const imgGallery = [img1, img2, img3, img4];
---

<ServiceLayout
  title={data.title}
  desc={data.desc}
  bannerImg={data.bannerImg}
  color={data.color}
  {imgGallery}
>
  <SupressaoContent />
</ServiceLayout>
